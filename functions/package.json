{
  "name": "pawcation-functions",
  "version": "1.0.0",
  "description": "Firebase Cloud Functions for Pawcation API",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "20"
  },
  "dependencies": {
    "@types/node": "^25.2.0",
    "axios": "^1.13.4",
    "busboy": "^1.6.0",
    "cors": "^2.8.6",
    "express": "^4.22.1",
    "firebase-admin": "^12.7.0",
    "firebase-functions": "^4.9.0",
    "form-data": "^4.0.0"
  },
  "devDependencies": {
    "@types/busboy": "^1.5.4",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "firebase-functions-test": "^3.1.0",
    "typescript": "^5.3.0"
  },
  "private": true
}
